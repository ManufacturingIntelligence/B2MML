      <xsd:element name = "MaterialDefinitionID"        type = "MaterialDefinitionIDType"
                                                        minOccurs = "0" maxOccurs = "unbounded"/>
      <xsd:element name = "MaterialTestSpecificationID" type = "MaterialTestSpecificationIDType"      
                                                        minOccurs = "0" maxOccurs = "unbounded"/>
      <xsd:element name = "AssemblyClassID"             type = "MaterialClassIDType"    
