                                                    minOccurs = "0" maxOccurs = "unbounded"/>
      <xsd:element name = "StartTime"               type = "StartTimeType"
                                                    minOccurs = "0"/>
      <xsd:element name = "EndTime"                 type = "EndTimeType"
                                                    minOccurs = "0"/>
      <xsd:element name = "PersonnelCapability"     type = "PersonnelCapabilityType" 
