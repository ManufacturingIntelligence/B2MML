                                                    minOccurs = "0"/>
      <xsd:element name = "PublishedDate"           type = "PublishedDateType"
                                                    minOccurs = "0"/>
      <xsd:element name = "CapabilityType"          type = "CapabilityTypeType" />
      <xsd:element name = "Reason"                  type = "ReasonType"
                                                    minOccurs = "0"/>
